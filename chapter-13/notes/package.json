    "on-server": "cross-env DEBUG=notes:* SEQUELIZE_CONNECT=models/sequelize-mysql.yaml NOTES_MODEL=sequelize TWITTER_CALLBACK_HOST=http://10.177.48.219:3000 USER_SERVICE_URL=http://10.177.48.138:5858 PORT=3000 node ./app.mjs",
